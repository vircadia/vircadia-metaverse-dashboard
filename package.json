        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@mdi/font": "^5.6.55",
        "core-js": "^3.6.4",
        "jquery": "^3.5.1",
        "roboto-fontface": "*",
